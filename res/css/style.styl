
li.L0, li.L1, li.L2, li.L3, li.L5, li.L6, li.L7, li.L8
  list-style-type decimal !important

body
  background #000
  color #aaa

#main
  min-height 1600px
  background #fff

#menus .lists a
  padding 10px 25px
  display block
  border-radius 5px
  &:hover
    background #ddd
    
  &.active
    background #08c
    color #eee
.badges
  a, span
    height 20px
    display inline-block
    vertical-align middle
    margin 0 4px 4px 0
  span
    margin-top 10px
.jumbotron
  background #fff

.max-100
  max-width 100%
.e-gif
  box-shadow 0px 0px 40px 3px rgba(0,0,0,0.75)
  border 3px solid #555
  border-radius 10px

#ubg
  position fixed
  left 0
  right 0
  top 0
  bottom 0
  background #000
  z-index 0
#content
  position relative
  z-index 1
  .list-group-item
    background none
    border-color #555
  .card
    border-color #555
    background #333
  .card-header
    background #333
    border-color #555
hr
  border-color #555
.big-logo
  background rgba(255, 255, 255, .3)
  border-radius 5px
  padding 5px