@require 'parts/prettify'

li.L0, li.L1, li.L2, li.L3, li.L5, li.L6, li.L7, li.L8
  list-style-type decimal !important

body
  background #fff

#main
  min-height 1600px
  background #fff

#menus .lists a
  padding 10px 25px
  display block
  border-radius 5px
  &:hover
    background #ddd
    
  &.active
    background #08c
    color #eee
.badges
  a, span
    height 20px
    display inline-block
    vertical-align middle
    margin 0 4px 4px 0
  span
    margin-top 10px
.jumbotron
  background #fff